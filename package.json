{
  "name": "mvvm-flutter",
  "displayName": "mvvm-flutter-nerdzlab",
  "description": "Simplifies the creation of MVVM architecture files in Flutter projects",
  "version": "1.1.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/nerdzlab/NerdzVSCode-MVVMGen"
  },
  "keywords": [
    "vscode",
    "flutter",
    "mvvm",
    "file generator",
    "dart"
  ],
  "publisher": "nerdzlab",
  "author": {
    "name": "NerdzLab"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:dart"
  ],
  "main": "./out/extension.js",
  "icon": "assets/logo.png",
  "contributes": {
    "commands": [
      {
        "command": "mvvm-flutter.new-view-model",
        "title": "MVVM: New ViewModel"
      },
      {
        "command": "mvvm-flutter.new-project",
        "title": "MVVM: New Project"
      },
      {
        "command": "mvvm-flutter.extract-to-arb",
        "title": "MVVM: Extract to ARB"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "mvvm-flutter.new-view-model",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/change-case": "^0.0.30",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.12",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "change-case": "^5.4.4",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21"
  }
}